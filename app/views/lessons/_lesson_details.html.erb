<div class="text-justify">
  <div >
    <div>
      <h5><%= t (".last_lesson") %></h5>
      <i class="text-muted">
        (<%= @lesson.last_lesson_time %> phút)
      </i>
    </div>
    <div>
      <p><%= raw @lesson.last_lesson_content %></p>
    </div>    
  </div>

  <div>
    <div>
      <h5><%= t (".object") %></h5>
    </div>
    <div>
      <p><%= raw @lesson.object %></p>
